{
  "expo": {
    "name": "my-app",
    "slug": "my-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/LogoTheBest.jpg",
    "backgroundColor": "#C44949",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/LogoTheBest.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#C44949"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.iismael.myapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "config": {
        "googleSignIn": {
          "expoClientId": " 619790277370-st8fno1oaglfom2d8ajvjmcfpmg07rk9.apps.googleusercontent.com"
        }
      }
    },
    "scheme": "my-app",
    "platforms": ["ios", "android", "web"],
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ]
  }
}

